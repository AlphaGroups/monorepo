{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/assets/*", "/images/*", "/api/*", "/_next/*" ]
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  },
  "routes": [
    {
      "route": "/lms/*",
      "redirect": "https://monorepo-lms.vercel.app/lms*",
      "statusCode": 301
    }
  ]
}
